    "babel-eslint": "6.1.2",
    "babel-jest": "15.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-react-app": "^0.2.1",
    "babel-preset-stage-3": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "concurrently": "3.0.0",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.0",
    "css-loader": "0.24.0",
    "detect-port": "1.0.0",
    "dotenv": "2.0.0",
    "enzyme": "^2.8.2",
    "eslint": "3.5.0",
    "eslint-config-react-app": "^0.2.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-flowtype": "2.18.1",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.22.0",
    "http-proxy-middleware": "0.17.1",
    "jest": "15.1.1",
    "jest-enzyme": "2.0.0",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "0.13.0",
    "promise": "7.1.1",
    "react-addons-test-utils": "15.4.2",
    "react-dev-utils": "^0.2.1",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "15.4.2",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "whatwg-fetch": "1.0.0",
    "@storybook/react": "^3.1.3"
  },
  "dependencies": {
    "firebase": "4.1.1",
    "material-ui": "0.18.4",
    "prop-types": "15.5.10",
    "raven-js": "3.15.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "4.4.8",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.8",
    "react-tap-event-plugin": "2.0.0",
    "react-transition-group": "1.1.3",
    "redux": "3.6.0",
    "redux-logger": "^3.0.6",
    "redux-responsive": "^4.3.2",
    "redux-thunk": "2.2.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "node scripts/build.js",
    "styles": "stylus -u autoprefixer-stylus ./src/styles/style.styl -o ./src/styles/style.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/styles/style.styl -o ./src/styles/style.css",
    "deploy-now": "ns ./build --single",
    "test": "node scripts/test.js --env=jsdom",
    "e2e": "cd e2e && ./node_modules/.bin/wdio ./wdio.conf.js && cd ..",
    "e2e-no-auth": "cd e2e && ./node_modules/.bin/wdio ./wdio.conf.js --spec specs/navigation.e2e.js && cd .."
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
